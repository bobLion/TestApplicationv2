<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <declare-styleable name="ExpandableTextView">
        <attr name="expandable" format="boolean" />
        <attr name="expand" format="boolean" />
        <attr name="collapse_lines" format="integer" />
    </declare-styleable>
    <declare-styleable name="InnerDrawableEditText">
        <attr name="rightIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>

    <declare-styleable name="FlikerProgressBar">
        <attr name="filk_textSize" format="dimension"/>
        <attr name="loadingColor" format="color"/>
        <attr name="stopColor" format="color"/>
        <attr name="filk_radius" format="dimension"/>
        <attr name="filk_borderWidth" format="dimension"/>
    </declare-styleable>
</resources>